{
  "name": "postgres-docker",
  "version": "1.0.0",
  "description": "Node Server with postgres and docker ",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "webpack --mode=production --config webpack.config.server.js",
    "start": "npm run build && NODE_ENV=production node ./dist/server.generated.js",
    "clean": "rimraf dist",
    "lintFix": "eslint --fix ./src",
    "prettier": "prettier --write ./src",
    "lint": "eslint ./src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vd89/node-postgress-docker.git"
  },
  "keywords": [
    "Nodejs",
    "express",
    "PostgresSQL",
    "docker",
    "modules"
  ],
  "engines": {
    "node": "v16.15.1",
    "npm": "8.12.1",
    "yarn": "1.22.19"
  },
  "author": "Dixit Vora <dixitvora@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vd89/node-postgress-docker/issues"
  },
  "homepage": "https://github.com/vd89/node-postgress-docker#readme",
  "dependencies": {
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "i18n": "^0.15.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
